{
    "name": "code4business/zugferd-php",
    "description": "ZUGFeRD PHP SDK - Convert PHP Objects to XML and back.",
    "keywords": [
        "ZUGFeRD"
    ],
    "type": "library",
    "minimum-stability": "stable",
    "version": "0.1.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Romowicz",
            "email": "patrick@romowicz.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "jms/serializer": "^3.4||^1",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Easybill\\ZUGFeRD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Easybill\\ZUGFeRD\\": "tests/"
        }
    }
}
